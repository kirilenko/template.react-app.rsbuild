{
  "name": "template.react-app.rsbuild",
  "author": "Kirilenko <n@kirilenko.com>",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "setup": "./scripts/nvm-use.sh && pnpm i && pnpm audit",
    "dev": "rsbuild dev --open",
    "eslint": "eslint . --fix 'src/**/**.{ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "pretty": "prettier --write 'src/**/**.{ts,tsx,css,style.ts}'",
    "format": "pnpm eslint && pnpm pretty && scripts/postformat.sh",
    "test": "NODE_NO_WARNINGS=1 jest",
    "test:ci": "NODE_NO_WARNINGS=1 jest --ci",
    "build:dev": "rsbuild build --env-mode development",
    "build:stage": "rsbuild build --env-mode staging",
    "build:main": "rsbuild build --env-mode production",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.0.4",
    "@rsbuild/plugin-eslint": "^1.0.3",
    "@rsbuild/plugin-react": "^1.0.2",
    "@swc/core": "^1.4.8",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
